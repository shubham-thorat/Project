extends base.pug 

block scripts
  script(src='/static/jscript/index.js')

block style
    style
      include ../static/css/style.css
      include ../static/css/styleContact.css
      
block content 
    div.container
        h1 Contact Us
        form(action="/contact" method="post" id="form")
            input(type="text", class="myInput", name="name", placeholder="Enter your name") 
            input(type="text", class="myInput", name="age", placeholder="Enter your Age") 
            input(type="text", class="myInput" ,name="gender" ,placeholder="Enter your gender") 
            input(type="text", class="myInput" ,name="mobile", placeholder="Enter your Mobile No")
            input(type="submit", value="submit" id="btn")
